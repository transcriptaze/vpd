{
  "name": "vpd",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_command"
      }
    },
    "_command": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "new"
        },
        {
          "type": "STRING",
          "value": "set"
        },
        {
          "type": "STRING",
          "value": "move"
        },
        {
          "type": "STRING",
          "value": "delete"
        }
      ]
    },
    "new": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "new"
        },
        {
          "type": "SYMBOL",
          "name": "entity"
        }
      ]
    },
    "entity": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "module"
        },
        {
          "type": "STRING",
          "value": "param"
        },
        {
          "type": "STRING",
          "value": "input"
        },
        {
          "type": "STRING",
          "value": "output"
        },
        {
          "type": "SYMBOL",
          "name": "label"
        }
      ]
    },
    "module": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "module"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "SYMBOL",
          "name": "height"
        },
        {
          "type": "SYMBOL",
          "name": "width"
        }
      ]
    },
    "name": {
      "type": "PATTERN",
      "value": "\"[a-zA-Z](.+?)\""
    },
    "height": {
      "type": "STRING",
      "value": "1U"
    },
    "width": {
      "type": "PATTERN",
      "value": "[1-9][0-9]*H"
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "label"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "\"(.+?)\""
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

